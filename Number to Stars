//This Java program takes five numbers as input and prints a series of stars (*) corresponding to each number. 

import java.util.Scanner;
public class Nums2Stars {

    public static void main(String[] args) {
     Scanner keyboard = new Scanner(System.in);
        System.out.print("Enter five numbers: ");
        int[] num = new int[5];
        
        for (int i = 0; i < 5; i++) {
            num[i] = keyboard.nextInt();
        }
        for (int i = 0; i < 5; i++) {
            for (int j = 0; j < num[i]; j++) {
                System.out.print("*");
            }
            System.out.println();
           
        }
    }
}
